{
  "name": "fetch-you",
  "version": "0.4.0",
  "description": "Fetch apstraction",
  "main": "dist/esm/main.js",
  "browser": "dist/esm/main.js",
  "module": "lib/main.mjs",
  "files": [
    "dist",
    "lib"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__",
    "example": "example"
  },
  "jspm": {
    "dependencies": {
      "node-fetch": "^2.6.0",
      "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
      "@jspm/core": "^1.0.4"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fusionstrings/fetch-you.git"
  },
  "author": "Dilip Kr. Shukla <contact@fusionstrings.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fusionstrings/fetch-you/issues"
  },
  "homepage": "https://github.com/fusionstrings/fetch-you#readme",
  "scripts": {
    "clean": "rm -rf dist",
    "test": "jest --detectOpenHandles",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production rollup -c",
    "server": "node example/server",
    "prepared": "npm run build && npm run test",
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "jb": "jspm build ./lib/fetch-you-browser.mjs"
  },
  "keywords": [
    "fetch",
    "rest",
    "json"
  ],
  "peerDependencies": {
    "jest-puppeteer": "^4.2.0",
    "puppeteer": "^1.18.1"
  },
  "devDependencies": {
    "@jspm/core": "^1.0.4",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "tridev": "^1.0.0"
  },
  "type": "module"
}
